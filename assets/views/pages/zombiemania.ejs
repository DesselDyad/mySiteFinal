<!DOCTYPE html>
<html>
<% include ../partials/head %>
    <body>
        <% include ../partials/header %>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <% include ../partials/locals_errors %>
                    <% include ../partials/form_errors %>
                </div>
                <div class="col-xs-3" id="sidebar_div">
                    <% include ../partials/zombie_sidebar %>
                </div>
                <div class="col-xs-9">
                    <div>
                        <%- zombie_content %>
                        <% if(typeof equipment != 'undefined') { %>
                            <% if(equipment.length) { %>
                                <% if(equipment.length > 0) { %>
                                    <% equipment.forEach(type => { %>
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <h1 class="equipment_title">
                                                    <i class="<%= type.category.icon_class %>" aria-hidden="true"> 
                                                    </i>
                                                    <%= type.category.name %>
                                                </h1>
                                                <table class="zombie_table">
                                                    <thead>
                                                        <tr>
                                                            <th>
                                                                Image
                                                            </th>
                                                            <th>
                                                                Item
                                                            </th>
                                                            <th>
                                                                Description
                                                            </th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% type.equipment.forEach(item => { %>
                                                            <tr>
                                                                <td>
                                                                    <a href="<%= '/zombiemania/equipment/' + item._id %>" title="<%= '/zombiemania/equipment/' + item._id %>">
                                                                        
                                                                    </a>
                                                                    <img src="<%= '/img/equipment/' + item.img %>" alt="<%= '/img/equipment/' + item.img %>" title="<%= '/img/equipment/' + item.img %>" class="img-responsive">
                                                                </td>
                                                                <td>
                                                                    <%= item.name %>
                                                                </td>
                                                                <td>
                                                                    <%= item.description %>
                                                                </td>
                                                            </tr>
                                                        <% }) %>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    <% }) %>
                                <% } %>
                            <% } %>
                        <% } %>
                        <% if(typeof movies != 'undefined') { %>
                            <% if(movies) { %>
                                <table class="zombie_table">
                                    <thead>
                                        <tr>
                                            <th>Title</th>
                                            <th>Director</th>
                                            <th>Year</th>
                                            <th>Notes</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% movies.forEach(movie => { %>
                                            <tr>
                                                <td>
                                                    <%= movie.title%>
                                                </td>
                                                <td>
                                                    <%= movie.director %>
                                                </td>
                                                <td>
                                                    <%= movie.year %>
                                                </td>
                                                <td>
                                                    <%= movie.notes%>
                                                </td>
                                            </tr>    
                                        <% }) %>
                                    </tbody>
                                </table>
                            <% } %>
                        <% } %>
                    </div>
                    
